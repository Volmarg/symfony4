{% extends 'base.html.twig' %}

{% block title %}Hello AdminDashboardController!{% endblock %}

    {% block stylesheets %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ asset('css/avatar.css') }}">
    {% endblock %}

{% block body %}
    {{ parent() }}
    <style>
        .example-wrapper {
            margin: 1em auto;
            max-width: 800px;
            width: 95%;
            font: 18px/1.5 sans-serif;
        }

        .example-wrapper code {
            background: #F5F5F5;
            padding: 2px 6px;
        }
    </style>

    {% include 'components/avatar/avatar.html.twig' %}

    <div class="example-wrapper">
        {% include 'components/admin/dashlets/users.html.twig' with {all_users:all_users} %}
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/avatar.js') }}"></script>
{% endblock %}
